$TTL    604800
@       IN      SOA     localhost. root.localhost. (
                              1         ; Serial
                         604800         ; Refresh
                          86400         ; Retry
                        2419200         ; Expire
                         604800 )       ; Negative Cache TTL
;
@       IN      NS      localhost.
1       IN      PTR     host1.domain.example.
2       IN      PTR     host2.domain.example.
3       IN      PTR     host3.domain.example.

10      IN      PTR     wrong.domain.example.

22      IN      PTR     test.domain.example.

128     IN      PTR     host1.domain2.example.
129     IN      PTR     host2.domain2.example.
130     IN      PTR     host3.domain2.example.